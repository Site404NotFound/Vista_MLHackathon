<div id="headcont"class="container">
  <div id="heading" class="jumbotron">
    <h1 class="display-3"> <i class="fas fa-search"></i> 404 - Page Not Found</h1>
    <hr class="my-4">
      <table class="table table-dark">
        <thead>
          <tr>
            {{#each headers}}
              <th scope="col">{{this}}</th>
            {{/each}}
          </tr>
        </thead>
        <tbody>
          {{#each dataList}}
          <tr>
            {{#each this}}
            <td>{{this}}</td>
            {{/each}}
          </tr>
          {{/each}}
        </tbody>
      </table>
  </div>
</div>